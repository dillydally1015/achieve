<div class="form">
  <div class="form_left">
    <%= fa_icon("user-circle 4x") %>
  </div>
  <div class="form_right">
    <%= form_with(model:@tukutter, local: true) do |form| %>
      <div class="post_content">
        <%= form.text_area :content %>
      </div>
      <div class="btn post_button">
        <%= form.submit "ツイート", :class => 'btn_submit' %>
      </div>
    <% end %>
  </div>
</div>
